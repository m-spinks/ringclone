<div class="col-md-3"></div>
<div class="col-md-6">
	<h2>Are you sure you want to cancel your subscription?</h2>
	<div class="col-md-6">
		<a href="@Url.Action("Index","Log")" class="btn btn-primary btn-block">No, take me back!</a>
	</div>
	<div class="col-md-6">
		<form action="/Account/CancelConfirm" method="POST">
			@Html.AntiForgeryToken()
			<input type="submit" value="Yes, cancel my subscription" class="btn btn-primary btn-block" style="white-space:normal;" />
		</form>
	</div>
</div>
<div class="col-md-3"></div>
